 "raw_recipe": "{% set name = \"lal\" %}\n{% set version = \"6.19.0\" %}\n{% set sha256 = \"729934a2f403b13f9c4f98d4d5e5d7fa4ea4e1158994165b42acc7e8149b4509\" %}\n\npackage:\n  name: {{ name }}\n  version: {{ version }}\n\nsource:\n  fn: {{ name }}-{{ version }}.tar.xz\n  url: http://software.ligo.org/lscsoft/source/lalsuite/{{ name }}-{{ version }}.tar.xz\n  sha256: {{ sha256 }}\n\nbuild:\n  number: 1004\n  skip: true  # [win]\n\nrequirements:\n  build:\n    - {{ compiler('c') }}\n    - pkg-config\n    - make\n    - bc\n    - swig >=3.0.7\n  host:\n    - blas 1.1 openblas\n    - openblas\n    - gsl\n    - zlib\n    - fftw\n    - hdf5\n\noutputs:\n  - name: lal\n    script: install-c.sh\n    build:\n      activate_in_script: True\n    requirements:\n      build:\n        - {{ compiler('c') }}\n        - pkg-config\n        - make\n        - bc\n        - swig >=3.0.7\n      host:\n        - blas 1.1 openblas\n        - openblas\n        - gsl\n        - zlib\n        - fftw\n        - hdf5\n      run:\n        - blas 1.1 openblas\n        - openblas\n        - gsl\n        - zlib\n        - fftw\n        - hdf5\n    test:\n      commands:\n        - lal_version --verbose\n        - lal_simd_detect\n    about:\n      home: https://wiki.ligo.org/DASWG/LALSuite\n      doc_url: https://lscsoft.docs.ligo.org/lalsuite/lal/\n      dev_url: https://git.ligo.org/lscsoft/lalsuite/\n      license: GPLv3\n      license_family: GPL\n      license_file: COPYING\n      summary: LSC Algorithm Library\n      description: |\n        The LSC Algorithm Library for gravitational wave data analysis.\n        This package contains the shared-object libraries and python bindings\n        needed to run applications that use the LAL library.\n  - name: python-lal\n    script: install-python.sh\n    build:\n      activate_in_script: True\n    requirements:\n      build:\n        - {{ compiler('c') }}\n        - make\n        - pkg-config\n        - swig >=3.0.7\n      host:\n        - {{ pin_