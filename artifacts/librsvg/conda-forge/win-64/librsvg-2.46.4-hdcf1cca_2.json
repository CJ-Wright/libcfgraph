 "raw_recipe": "{% set version = \"2.46.4\" %}\n{% set version_major = version.split('.', 1)[0] %}\n{% set version_majmin = version.rsplit('.', 1)[0] %}\n{% set sha256 = \"b45b9ee3b64c58baaf800bcdff5fcd04d79930dba4c56e46e0d3b0aead40cc29\" %}\n\npackage:\n  name: librsvg\n  version: {{ version }}\n\nsource:\n  fn: librsvg-{{ version }}.tar.xz\n  url: http://ftp.gnome.org/pub/gnome/sources/librsvg/{{ version_majmin }}/librsvg-{{ version }}.tar.xz\n  sha256: {{ sha256 }}\n  patches:\n    # can remove Visual-Studio-Fix-builds patch for librsvg 2.47.3+\n    - 0001-Visual-Studio-Fix-builds.patch  # [win]\n\nbuild:\n  number: 2\n  run_exports:\n    # Looks good but no new info after 2.43 so keeping it x.x for safety.\n    # https://abi-laboratory.pro/?view=timeline&l=librsvg\n    - {{ pin_subpackage('librsvg', max_pin='x') }}\n\nrequirements:\n  build:\n    - cmake  # [win]\n    - make  # [unix]\n    - pkg-config\n    - rust\n    - {{ compiler('c') }}\n  host:\n    - glib\n    - libiconv  # [win]\n    - libxml2\n    - cairo\n    - gdk-pixbuf\n    - gobject-introspection  # [win]\n    - libcroco 0.6.*\n    - pango\n  run:\n    - glib\n    - libiconv  # [win]\n    - libxml2\n    - cairo\n    - gdk-pixbuf\n    - libcroco 0.6.*\n    - pango\n\ntest:\n  requires:\n    - pkg-config\n  commands:\n    - rsvg-convert --version\n\n    {% set name = \"librsvg\" %}\n    {% set namever = \"librsvg-2.0\" %}\n    {% set lib = \"librsvg-2\" %}  # [unix]\n    {% set lib = \"rsvg-2.0\" %}  # [win]\n    {% set vs_major = dict(vs2015=\"14\", vs2017=\"15\", vs2019=\"16\")[c_compiler] %}  # [win]\n\n    # verify that libs get installed and can be located through pkg-config\n    - test -f $PREFIX/lib/{{ lib }}${SHLIB_EXT}  # [unix]\n    - test -f `pkg-config --variable=libdir --dont-define-prefix {{ namever }}`/{{ lib }}${SHLIB_EXT}  # [unix]\n    - if not exist %PREFIX%\\\\Library\\\\bin\\\\{{ lib }}-vs{{ vs_major }}.dll exit 1  # [win]\n    - for /f \"usebackq tokens=*\" %%a in (`pkg-config --variable=exec_prefix --dont-define-prefix {{ namever }}`) do if not exist \"%%a/bin/{{ lib }}-vs{{ vs_major }}.dll\" exit 1  # [win]\n    - if not exist %PREFIX%\\\\Library\\\\lib\\\\{{ lib }}.lib exit 1  # [win]\n    - for /f \"usebackq tokens=*\" %%a in (`pkg-config --variable=libdir --dont-define-prefix {{ namever }}`) do if not exist \"%%a/{{ lib }}.lib\" exit 1  # [win]\n\n    # verify that gdk-pixbuf loader library gets installed\n    - test -f $PREFIX/lib/gdk-pixbuf-2.0/2.10.0/loaders/libpixbufloader-svg.so  # [unix]\n    - if not exist %PREFIX%\\\\Library\\\\lib\\\\gdk-pixbuf-2.0\\\\2.10.0\\\\loaders\\\\libpixbufloader-svg.dll exit 1  # [win]\n\n    # verify that headers get installed\n    - test -f $PREFIX/include/{{ namever }}/{{ name }}/librsvg-features.h  # [unix]\n    - test -f $PREFIX/include/{{ namever }}/{{ name }}/rsvg-cairo.h  # [unix]\n    - test -f $PREFIX/include/{{ namever }}/{{ name }}/rsvg.h  # [unix]\n    - if not exist %PREFIX%\\\\Library\\\\include\\\\{{ namever }}\\\\{{ name }}\\\\librsvg-features.h exit 1  # [win]\n    - if not exist %PREFIX%\\\\Library\\\\include\\\\{{ namever }}\\\\{{ name }}\\\\rsvg-cairo.h exit 1  # [win]\n    - if not exist %PREFIX%\\\\Library\\\\include\\\\{{ namever }}\\\\{{ name }}\\\\rsvg.h exit 1  # [win]\n\nabout:\n  home: https://wiki.gnome.org/Projects/LibRsvg\n  license: LGPL-2.1-or-later\n  license_file: COPYING.LIB\n  summary: librsvg is a library to render SVG files using cairo.\n\nextra:\n  recipe-maintainers:\n    - jenzopr\n    - pkgw\n",
